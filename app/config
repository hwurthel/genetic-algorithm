out_file 	= result	-- Имя выходного файла
tmp_of   	= tmp_of	-- Имя выходной файла для обсчета параметров белка
tmp_if   	= tmp_if	-- Имя входного файла, где находятся обсчитанные параметры

time_wait 	= 5000000	-- Время задержки перед проверка файла @tmp_if@, в мкс

eval_param 	= 0.05		-- Параметр для функции оценки
pop_size   	= 10		-- Размер популяции
prob_cros  	= 0.5		-- Вероятность того, что хромосома будет участвовать в кроссинговере
prob_cros_gene  = 0.5		-- Вероятность того, что ген в хромосоме подвергнется кроссинговеру
prob_mut        = 0.5		-- Вероятность того, что хромосома будет участвовать в мутации
prob_mut_gene   = 0.5		-- Вероятность того, что ген в хромосоме подвергнется мутации
